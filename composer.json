{
  "name": "wepamultimedia/laravel-passport-api",
  "description": "",
  "keywords": ["Laravel"],
  "authors": [
    {
      "name": "Carlos Puche",
      "email": "cpuche@wepa.es"
    }
  ],
  "homepage": "https://github.com/wepamultimedia/laravel-passport-api",
  "require": {
    "php": "^8.0.2",
    "laravel/framework": "^9.5.1",
    "laravel/passport": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "^6.0",
    "phpstan/phpstan": "^0.12"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Wepamultimedia\\LaravelPassportApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wepamultimedia\\LaravelPassportApi\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wepamultimedia\\LaravelPassportApi\\Providers\\LaravelPassportApiServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit -c ./phpunit.xml --colors=always",
    "analysis": "vendor/bin/phpstan analyse"
  }
}
